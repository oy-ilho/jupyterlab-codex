{
  "name": "jupyterlab-codex-sidebar",
  "version": "0.1.5",
  "author": "ILHO AHN",
  "description": "Codex sidebar for JupyterLab",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oy-ilho/jupyterlab-codex.git"
  },
  "homepage": "https://github.com/oy-ilho/jupyterlab-codex#readme",
  "bugs": {
    "url": "https://github.com/oy-ilho/jupyterlab-codex/issues"
  },
  "keywords": [
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter"
  ],
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension",
    "build:lib": "tsc -p tsconfig.json",
    "build:labextension": "jupyter labextension build .",
    "watch": "jlpm run watch:lib & jlpm run watch:labextension",
    "watch:lib": "tsc -w -p tsconfig.json",
    "watch:labextension": "jupyter labextension watch . --development --source-map",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "test:unit": "playwright test --config playwright.unit.config.cjs",
    "test:e2e": "bash scripts/run_playwright_e2e.sh",
    "test:e2e:queue-repro": "bash scripts/run_playwright_queue_repro.sh",
    "test:e2e:queue-repro:local": "bash scripts/run_playwright_repro.sh",
    "test:e2e:headed": "bash scripts/run_playwright_e2e.sh --headed",
    "test:e2e:install": "playwright install chromium",
    "test:e2e:repro-local": "bash scripts/run_playwright_repro.sh"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@lumino/widgets": "^2.0.0",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.28",
    "marked": "^16.4.2",
    "marked-katex-extension": "^5.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@playwright/test": "^1.56.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "typescript": "~5.2.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_codex/labextension",
    "webpackConfig": "webpack.config.js"
  }
}
